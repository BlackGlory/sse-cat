{
  "name": "sse-cat",
  "version": "1.3.1",
  "description": "Command-line client for Server-Sent Events(SSE).",
  "keywords": [
    "CLI",
    "cat",
    "SSE",
    "Server-Sent Event",
    "Server-Sent Events"
  ],
  "bin": {
    "sse-cat": "lib/cli.js"
  },
  "files": [
    "lib"
  ],
  "repository": "git@github.com:BlackGlory/sse-cat.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src",
    "dev": "ts-node src/index.ts",
    "dev:debug": "node --inspect-brk node_modules/.bin/ts-node src/index.ts",
    "prepublishOnly": "run-s clean build",
    "clean": "rimraf lib",
    "build": "run-s build:*",
    "build:compile": "tsc --project tsconfig.build.json",
    "build:patch": "tscpaths -p tsconfig.build.json -s ./src -o ./lib",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s lint build",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "commander": "^7.1.0",
    "eventsource": "https://github.com/EventSource/eventsource#46fe04e03e54f4129a28bf75b3a1e5f4ab68b52a",
    "rxjs": "^7.0.0-beta.10"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.0",
    "@commitlint/config-conventional": "^12.0.0",
    "@types/eventsource": "^1.1.5",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "eslint": "^7.20.0",
    "husky": "^4.3.8",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "standard-version": "^9.1.1",
    "ts-node": "^9.1.1",
    "tscpaths": "^0.0.9",
    "typescript": "^4.2.2"
  }
}
