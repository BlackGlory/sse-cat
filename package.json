{
  "name": "sse-cat",
  "version": "2.0.5",
  "description": "Command-line client for Server-Sent Events(SSE).",
  "keywords": [
    "CLI",
    "cat",
    "SSE",
    "Server-Sent Event",
    "Server-Sent Events"
  ],
  "bin": {
    "sse-cat": "lib/cli.js"
  },
  "files": [
    "lib"
  ],
  "repository": "git@github.com:BlackGlory/sse-cat.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "scripts": {
    "prepare": "ts-patch install -s",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src",
    "test": "jest --config jest.config.js",
    "test:coverage": "jest --coverage --config jest.config.js",
    "dev": "ts-node -r tsconfig-paths/register src/cli.ts",
    "dev:debug": "node --inspect-brk node_modules/.bin/ts-node -r tsconfig-paths/register src/cli.ts",
    "prepublishOnly": "run-s prepare clean build",
    "clean": "rimraf lib",
    "build": "tsc --project tsconfig.build.json",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s prepare lint build test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@blackglory/errors": "^2.1.0",
    "@blackglory/go": "^0.1.0",
    "@blackglory/types": "^0.6.5",
    "commander": "^9.0.0",
    "eventsource": "^1.1.0",
    "extra-timers": "^0.2.5",
    "hotypes": "^0.4.3",
    "justypes": "^0.1.6",
    "rxjs": "^7.5.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@types/eventsource": "^1.1.8",
    "@types/jest": "^27.4.0",
    "@types/node": "14",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "eslint": "^8.8.0",
    "husky": "4",
    "jest": "^27.5.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "standard-version": "^9.3.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "ts-patch": "^2.0.1",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.5",
    "typescript-transform-paths": "^3.3.1"
  }
}
